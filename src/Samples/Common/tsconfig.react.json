{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "jsx": "react",
    "target": "ES6",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "ES6"
    ]
  },
  "include": [
    "Scripts/react/**/*"
  ],
  
  "exclude": [
    "node_modules",
    "wwwroot",
  ]
}
